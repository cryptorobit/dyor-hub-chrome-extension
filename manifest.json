{
  "name": "DYORHUB",
  "description": "DYORHUB Chrome extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "dyorhub.png"
  },
  "permissions": ["tabs","activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://gmgn.ai/*",
    "https://pump.fun/*",
    "https://dexscreener.com/*",
    "https://axiom.trade/*"
  ]
}
